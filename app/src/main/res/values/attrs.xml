<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CropImageView">

        <attr format="color" name="color"/>
        <attr format="color" name="borderColor"/>
        <attr format="dimension" name="border"/>
        <attr format="dimension" name="width"/>
        <attr format="dimension" name="height"/>
        <attr format="boolean" name="changeStyle"/>
        <attr format="enum" name="shape">
            <enum name="rectangle" value="0"/>
            <enum name="circle" value="1"/>
            <enum name="corner" value="2"/>
        </attr>

    </declare-styleable>

</resources>